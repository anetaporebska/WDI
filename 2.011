
#include <iostream>

using namespace std;


//2.011. Napisz program wczytujący liczbę naturalną z klawiatury i odpowiadający na pytanie, czy liczba ta jest wielokrotnościa
//dowolnego wyrazu ciągu danego wzorem a(n)=3*a(n-1)+1, a pierwszy wyraz jest równy 2;

int main(){
    
   int n;
   cin>>n;
   int wyraz;
   int wyrazpop=2;
   
   
   while(wyraz<n){
        if(n%wyrazpop==0) {cout<<"tak"; return 0;}
        wyraz=wyrazpop*3+1;
        wyrazpop=wyraz;
        
   }
  
    cout<<"nie";
    
    
    return 0;
}
