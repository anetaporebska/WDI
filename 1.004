#include <iostream>

using namespace std;

//1.004 Napisać program obliczający pierwiastek całkowitoliczbowy z liczby naturalnej korzystając z zależności 1+3+5+...=n^2



int main()
{
int a, wyn=0;
cin>>a;
int suma=1;
int liczba=1;


while(suma<=a)
{   
    liczba+=2;
    suma+=liczba;
    wyn++;
}
cout<<wyn;

    return 0;
}
