
#include <iostream>
using namespace std;


//A.014 Funkcja zamieniajÄ…ca inta na stringa.





string to_stringa(int n){
    
    string tmp,res;
    
    do{
        tmp+=n%10+48;
        n-=n%10;
    }
    while(n/=10);
    
    
    for(int i=tmp.size()-1;  i>=0; i--) res+=tmp[i];
    
    
    
    return res;
}


int main(){
    
    
    int n=1982;
    string str=to_stringa(n);
    cout<<str+"aneta";
    
    
    
    
    
    return 0;
}








