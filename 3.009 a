    n=o_ile_pol(n,wartosc);
    k+=n;
    if(!czy_w_obrebie) n
    
    
}


}

int main(){
    
    
    int t[N];
    for(int i=0; i<N; i++) {t[i]=rand()%50+1; cout<<t[i]<<" ";}
    
    if(przeskok(t)) cout<<"tak";
    else cout<<"nie";
    
}
